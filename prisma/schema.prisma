generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Name {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  species   String
  craetedAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("names")
}

model Owner {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  firstName String
  lastName  String
  craetedAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("owners")
}
